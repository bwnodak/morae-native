{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest",
    "react",
    "react-native"
  ],
  "rules": {
    "semi": [2, "never"],
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/valid-expect": "error",
    "jsx-a11y/anchor-is-valid": 2,
    "react/jsx-filename-extension": 0
  }
}
